{
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    ["syntax-dynamic-import"],
    ["transform-class-properties"],
    ["babel-plugin-styled-components", {
      "minify": false
    }]
  ],
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": "last 2 Chrome versions"
      }
    }],
    "@babel/react"
  ]
}